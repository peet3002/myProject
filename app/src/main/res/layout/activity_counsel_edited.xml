<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".CounselEditedActivity">

    <include
        android:id="@+id/counsel_edited_toolbar"
        layout="@layout/app_bar_layout">
    </include>

    <include layout="@layout/search_toolbar_layout" />


    <View
        android:id="@+id/line_speac"
        android:layout_height="3dip"
        android:layout_below="@+id/counsel_edited_toolbar"
        android:layout_width="match_parent"
        android:background="@color/colorPrimary"/>

    <FrameLayout
        android:id="@+id/main_container"
        android:layout_marginTop="5dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/line_speac"
        android:background="@color/colorWhite">

        <android.support.v7.widget.RecyclerView
            android:id="@+id/counsel_edited_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </FrameLayout>

</RelativeLayout>